<div class="container">
    <h4Recommend Tracks</h4>
    <div>
      <form action="/tracks/recommend" method="GET">
        <label>Title</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
  </div>
    <div class="container">
    <button class="btn btn-info" type="submit">Search</button>
    </form>
    <br>
    <br> 
    <p>Same title</p>
    <% tracks.forEach(track => { %>
      <a href="/tracks/<%= track.id %>">
    <img class="d-inline"  src="<%= track.coverImagePath %> " height="75" width="75">
      </a>  
      <a href="/tracks/<%= track.id %>"><h5 class="d-inline"  ><%= track.title %> </h5></a>
      <a href="/tracks/<%= track.id %>"><h5 class="d-inline"  ><%= track.genre %> </h5></a>
    <br>
    <% }) %>
    <p>same genre</p>
    <% cursor.forEach(track => { %>
      <a href="/tracks/<%= track.id %>">
    <img class="d-inline"  src="<%= track.coverImagePath %> " height="75" width="75">
      </a>  
      <a href="/tracks/<%= track.id %>"><h5 class="d-inline"  ><%= track.title %> </h5></a>
      <a href="/tracks/<%= track.id %>"><h5 class="d-inline"  ><%= track.genre %> </h5></a>
    <br>
    <% }) %>
  </div>