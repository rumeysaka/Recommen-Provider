<div class="container">
<h2>Search Artists</h2>

<form action="/artists" method="GET">
<label >Name</label>
<input type="text" name="name" value="<%= searchOptions.name %> ">
<button class="btn btn-info" type="submit">Search</button>
</form>
<br>
<% artists.forEach(artist =>{ %> 
    <div><% artist.name %> </div>
    <% })%> 
</div>